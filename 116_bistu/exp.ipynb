   "source": [
    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv('teacher_detail.csv')\n",
    "cols = df.columns.tolist()\n",
    "\n",
    "arranged_cols = sorted(\n",
    "    cols, \n",
    "    key=lambda x: df[x].isnull().sum()\n",
    ")\n",
    "\n",
    "df = df[arranged_cols]\n",
    "df.to_csv('teacher_detail.csv', index=False, encoding='utf-8-sig')"
   ],
